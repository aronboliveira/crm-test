{
  "name": "mobile",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "CRM Mobile App - React Native",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "type-check": "tsc --noEmit",
    "cypress": "cypress run --config-file cypress.config.ts",
    "cypress:open": "cypress open --config-file cypress.config.ts",
    "cypress:run:smoke": "cypress run --config-file cypress.config.ts --spec cypress/e2e/smoke.cy.ts",
    "test:e2e:mobile": "npm run cypress:run:smoke && npm run detox:doctor",
    "detox:doctor": "detox doctor",
    "detox:build:ios": "detox build -c ios.sim.debug",
    "detox:test:ios": "detox test -c ios.sim.debug",
    "detox:build:android": "detox build -c android.emu.debug",
    "detox:test:android": "detox test -c android.emu.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-picker/picker": "^2.11.4",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "axios": "^1.6.0",
    "events": "^3.3.0",
    "react": "^19.2.4",
    "react-native": "^0.83.1",
    "react-native-mmkv": "^4.1.2",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.23.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-syntax-jsx": "^7.28.6",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-react-jsx": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@react-native/babel-preset": "0.83.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "@types/uuid": "^9.0.7",
    "babel-jest": "^29.7.0",
    "cypress": "^15.10.0",
    "detox": "^20.40.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^19.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
